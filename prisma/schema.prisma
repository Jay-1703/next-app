generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int    @id @default(autoincrement())
  username String @db.VarChar(200)
  email    String @unique(map: "User_email_key") @db.VarChar(200)
  password String @db.VarChar(200)
  number   Int 
}
